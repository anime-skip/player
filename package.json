{
  "name": "anime-skip-player",
  "version": "1.1.5",
  "private": true,
  "author": "Aaron Klinker <aaronklinker1@gmail.com>",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "compile": "vue-tsc --noEmit",
    "lint": "eslint . --ext .js,.ts,.vue --max-warnings 0 --fix",
    "lint:no-fix": "eslint . --ext .js,.ts,.vue --max-warnings 0",
    "prettier": "prettier --check \"src/**/*.{js,ts,vue}\"",
    "prettier:write": "yarn prettier --write",
    "check-manifest": "yarn build --mode prod && yarn web-ext --config=.web-ext.config.js lint && yarn build --mode prod --for chrome && yarn web-ext --config=.web-ext.config.js lint",
    "package": "node scripts/package.js",
    "package:dry": "DRY_RUN=true yarn package",
    "staged-zip": "mkdir -p artifacts; rm -f artifacts/staged.zip && cd dist && zip -r ../artifacts/staged.zip * ; cd ..",
    "build": "./webpack.js --build",
    "build:firefox-staged": "yarn build --mode staged && yarn staged-zip",
    "build:chrome-staged": "yarn build --mode staged --for chrome && yarn staged-zip",
    "zip:sources": "node -e 'require(\"./scripts/zip-sources\")(\"artifacts\")'",
    "start": "./webpack.js",
    "test": "jest --runInBand",
    "test:watch": "yarn test --watchAll",
    "test:e2e": "echo 'TODO'",
    "webpack-profile": "./webpack.js --profile > stats.json && echo 'Upload ./stats.json to https://alexkuz.github.io/webpack-chart/'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@anime-skip/ui": "^0.2.2",
    "axios": "^0.19.0",
    "fuzzysort": "^1.1.4",
    "md5": "^2.2.1",
    "vue": "^3.0.4",
    "vuex": "^4.0.0",
    "webextension-polyfill": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime-corejs3": "^7.4.0",
    "@types/fs-extra": "^9.0.2",
    "@types/jest": "^26.0.7",
    "@types/md5": "^2.1.33",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "archiver": "^3.0.0",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.0.1",
    "css-loader": "^3.4.0",
    "dotenv": "^8.2.0",
    "ejs": "^2.6.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^7.7.0",
    "file-loader": "^5.0.2",
    "form-data": "^3.0.0",
    "fs-extra": "^9.0.1",
    "husky": "^4.3.0",
    "jest": "^26.2.1",
    "jest-mocha-reporter": "^1.0.1",
    "mini-css-extract-plugin": "^1.3.8",
    "postcss": "^8.2.6",
    "postcss-loader": "^5.0.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^1.8.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "tailwindcss": "^2.0.3",
    "ts-jest": "^26.1.4",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "vue-loader": "^16.1.2",
    "vue-tsc": "^0.0.14",
    "web-ext": "^5.5.0",
    "web-ext-types": "^3.2.1",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-extension-reloader": "^1.1.4",
    "webpack-shell-plugin-next": "^2.1.1",
    "yargs": "^16.2.0",
    "zip-a-folder": "^0.0.12"
  }
}
