{
  "name": "anime-skip-web-ext",
  "version": "1.0.13",
  "private": true,
  "author": "Aaron Klinker <aaronklinker1@gmail.com>",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "compile": "tsc -p . --noEmit",
    "lint": "eslint --ext .js,.ts,.vue src --max-warnings=0",
    "prettier": "prettier --check \"src/**/*.{js,ts,vue}\"",
    "prettier:write": "yarn prettier --write",
    "check-manifest": "yarn build:prod && yarn web-ext --config=.web-ext.config.js lint && yarn build:chrome:prod && yarn web-ext --config=.web-ext.config.js lint",
    "package": "./scripts/package.sh",
    "build": "cross-env BUILD_ONLY=true BUILD_FOR=firefox NODE_ENV=development webpack --hide-modules --watch",
    "build:prod": "cross-env BUILD_ONLY=true BUILD_FOR=firefox NODE_ENV=production webpack --hide-modules",
    "build:chrome": "cross-env BUILD_ONLY=true BUILD_FOR=chrome NODE_ENV=development webpack --hide-modules --watch",
    "build:chrome:prod": "cross-env BUILD_ONLY=true BUILD_FOR=chrome NODE_ENV=production webpack --hide-modules",
    "start": "cross-env HMR=true BUILD_FOR=firefox NODE_ENV=development webpack --hide-modules --watch",
    "start:prod": "cross-env BUILD_FOR=firefox NODE_ENV=production webpack --hide-modules",
    "start:chrome": "cross-env HMR=true BUILD_FOR=chrome NODE_ENV=development webpack --hide-modules --watch",
    "start:chrome:prod": "cross-env BUILD_FOR=chrome NODE_ENV=production webpack --hide-modules",
    "test": "jest --runInBand",
    "test:watch": "yarn test --watchAll",
    "test:integration": "echo 'TODO'",
    "test:e2e": "echo 'TODO'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "axios": "^0.19.0",
    "fuzzysort": "^1.1.4",
    "md5": "^2.2.1",
    "vue": "2.6.11",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-ripple-directive": "^2.0.1",
    "vue-router": "^3.0.1",
    "vue-slider-component": "^3.2.2",
    "vuex": "^3.0.1",
    "webextension-polyfill": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime-corejs3": "^7.4.0",
    "@types/jest": "^26.0.7",
    "@types/md5": "^2.1.33",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "archiver": "^3.0.0",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.4.0",
    "dotenv": "^8.2.0",
    "ejs": "^2.6.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "file-loader": "^5.0.2",
    "husky": "^2.4.0",
    "jest": "^26.2.1",
    "jest-mocha-reporter": "^1.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.9.3",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.8.0",
    "sass-loader": "^7.1.0",
    "ts-jest": "^26.1.4",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.6.10",
    "web-ext": "^4.3.0",
    "web-ext-types": "^3.2.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.3.10",
    "webpack-extension-reloader": "^1.1.0",
    "webpack-shell-plugin-next": "^1.1.5"
  }
}
