{
  "typescript.tsdk": "node_modules/typescript/lib",
  "volar.tsPlugin": true,
  "volar.tsPluginStatus": false,
  "vite.autoStart": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": true
  },
  "files.associations": {
    "*.css": "postcss",
    ".env.package": "dotenv"
  },
  "[vue]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false
  },
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false
  },
  "editor.rulers": [100],
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "cSpell.words": [
    "aklinker",
    "callout",
    "chika",
    "crunchyroll",
    "entrypoint",
    "entrypoints",
    "focusable",
    "fullscreen",
    "funimation",
    "gzip",
    "gzips",
    "klinker",
    "lookin",
    "mixin",
    "mixins",
    "outro",
    "outros",
    "prefill",
    "skippable",
    "unitless",
    "unmount",
    "vrv",
    "vuex"
  ],
  "cSpell.ignoreWords": [
    "asynciterable",
    "builtins",
    "camelcase",
    "webextension",
    "webextensions",
    "lrgdkj",
    "fuzzysort",
    "corejs",
    "parens",
    "vilos",
    "softsubs",
    "brightcove",
    "konqueror",
    "edgechromium",
    "endregion",
    "vite",
    "demi"
  ],
  "css.lint.unknownAtRules": "ignore",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
    // "**/components.d.ts": true
  },
  "workbench.editorAssociations": {
    ".env.package": "dotenv"
  }
}
