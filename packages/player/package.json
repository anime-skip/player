{
  "name": "@anime-skip/player",
  "version": "2.0.0",
  "description": "Video player integrated with Anime Skip",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "compile": "vue-tsc --noEmit",
    "test": "vitest",
    "generate:graphql": "graphql-codegen --config graphql-codegen.yml",
    "prepare": "pnpm build"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@graphql-codegen/typescript-graphql-request": "^4.5.9",
    "@graphql-codegen/typescript-operations": "^3.0.4",
    "@iconify/json": "^2.2.162",
    "@types/chance": "^1.1.6",
    "@types/md5": "^2.3.5",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "chance": "^1.1.11",
    "daisyui": "^2.52.0",
    "postcss": "^8.4.32",
    "postcss-unroot": "^1.0.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-icons": "^0.16.6",
    "vite": "^5.0.10",
    "vite-plugin-css-injected-by-js": "^3.3.1",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.1.0",
    "vue-tsc": "^1.8.27"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.1",
    "fuzzysort": "^2.0.4",
    "graphql": "^16.8.1",
    "graphql-request": "~5.1.0",
    "graphql-tag": "^2.12.6",
    "md5": "^2.3.0",
    "p-singleton": "^1.0.4",
    "uuid": "^9.0.1",
    "vue": "^3.3.13",
    "vue-query": "^1.26.0"
